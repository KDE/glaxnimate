    <hint type="destinationlabel">
     <x>132</x>
     <y>398</y>
    </hint>
   </hints>
  </connection>
  <connection>
   <sender>btn_clear_console</sender>
   <signal>clicked()</signal>
   <receiver>ScriptConsole</receiver>
   <slot>console_clear()</slot>
   <hints>
    <hint type="sourcelabel">
     <x>202</x>
     <y>262</y>
    </hint>
    <hint type="destinationlabel">
     <x>218</x>
     <y>352</y>
    </hint>
   </hints>
  </connection>
 </connections>
